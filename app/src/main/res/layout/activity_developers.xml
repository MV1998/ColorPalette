                        <ImageView
                            android:id="@+id/mohit_linkedin_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/linkedin" />

                    </LinearLayout>

                </RelativeLayout>


            </android.support.v7.widget.CardView>


